# Copy this file to `.env` and fill in real values. Do NOT commit secrets.

# Supabase PostgreSQL Connection
DATABASE_URL=postgres://username:password@host:port/database

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Optional: Supabase service role key (server-side uploads, migrations)
# Provide this to enable server-side uploads to Supabase Storage. Keep it secret.
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional: Google Drive API
GOOGLE_DRIVE_API_KEY=your-google-drive-api-key
VITE_GOOGLE_DRIVE_API_KEY=your-google-drive-api-key
